public class Main{
public static void main(String[] args){
     int mes = 12;
      
      switch (mes) {
case 1:
System.out.println("trata-se do mês de janeiro");
break;
case 2:
System.out.println("trata-se do mes fevereiro");
break;
case 3:
System.out.println("trata-se do mes março");
break;
case 4:
System.out.println("trata-se do mes abril");
break;
case 5:
System.out.println("trata-se do mes maio ");
break;
case 6:
System.out.println("trata-se do mes junho");
break;
case 7:
System.out.println("trata-se do mes julho");
break;
case 8:
System.out.println("trata-se do mes agosto");
break;
case 9:
System.out.println("trata-se do mes setembro");
break;
case 10:
System.out.println("trata-se do mes outobro");
break;
case 11:
System.out.println("trata-se do mes novembro");
break;
case 12:
System.out.println("trata-se do mes dezenbro");
break;
default:
System.out.println("mês inexistente");
break;
}
}
}
